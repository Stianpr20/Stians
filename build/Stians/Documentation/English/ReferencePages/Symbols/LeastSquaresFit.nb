     RowBox[{"a", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"sumx2", "*", "sumy"}], "-", 
         RowBox[{"sumx", "*", "sumxy"}]}], ")"}], "/", "\[CapitalDelta]"}]}], 
     ";", "\[IndentingNewLine]", "\t", 
     RowBox[{"b", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"sumw", "*", "sumxy"}], "-", 
         RowBox[{"sumx", "*", "sumy"}]}], ")"}], "/", "\[CapitalDelta]"}]}], 
     ";", "\[IndentingNewLine]", "\[IndentingNewLine]", "\t", 
     RowBox[{"(*", " ", "Uncertainties", " ", "*)"}], "\[IndentingNewLine]", 
