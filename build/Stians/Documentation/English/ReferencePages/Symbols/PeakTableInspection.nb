            RowBox[{
             RowBox[{"\[Phi]", "=", 
              RowBox[{"top", "[", 
               RowBox[{"[", 
                RowBox[{"j", ",", "5"}], "]"}], "]"}]}], ";", 
             "\[IndentingNewLine]", "\t\t", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{
               "IntervalMemberQ", "[", "\[IndentingNewLine]", "\t\t", 
                RowBox[{
                 RowBox[{"Interval", "[", 
                  RowBox[{
                   RowBox[{
                    RowBox[{"{", 
                    RowBox[{
                    RowBox[{"\[Phi]max", "-", "#"}], ",", 
                    RowBox[{"\[Phi]max", "+", "#"}]}], "}"}], "&"}], "[", 
                   "width", "]"}], "]"}], ",", "\[Phi]"}], "]"}], ",", 
               "\[IndentingNewLine]", "\t\t", 
               RowBox[{"AppendTo", "[", 
                RowBox[{"groups", ",", 
                 RowBox[{"top", "[", 
                  RowBox[{"[", "j", "]"}], "]"}]}], "]"}]}], "]"}]}], ",", 
            "\[IndentingNewLine]", "\t\t", 
            RowBox[{"{", 
             RowBox[{"j", ",", 
              RowBox[{"Length", "@", "top"}]}], "}"}]}], "]"}], ";", 
          "\[IndentingNewLine]", "\[IndentingNewLine]", "\t", 
          RowBox[{"(*", " ", 
           RowBox[{
           "Clear", " ", "group", " ", "from", " ", "the", " ", "set", " ", 
            "and", " ", "repeat"}], " ", "*)"}], "\[IndentingNewLine]", 
          "\t\t", 
          RowBox[{"top", "=", 
           RowBox[{"Complement", "[", 
            RowBox[{"top", ",", "groups"}], "]"}]}], ";", 
          "\[IndentingNewLine]", "\t\t", 
          RowBox[{"AppendTo", "[", 
           RowBox[{"newset", ",", 
            RowBox[{"{", "groups", "}"}]}], "]"}], ";", "\[IndentingNewLine]",
           "\t\t", 
          RowBox[{"groups", "=", 
           RowBox[{"{", "}"}]}]}]}], "\[IndentingNewLine]", "\t", "]"}], ",", 
       "\[IndentingNewLine]", "\t", 
       RowBox[{"{", 
        RowBox[{"j", ",", 
         RowBox[{"Length", "@", "top"}]}], "}"}]}], "\[IndentingNewLine]", 
