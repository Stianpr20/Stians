          RowBox[{
           RowBox[{"#", "[", 
            RowBox[{"[", "6", "]"}], "]"}], "&"}], ",", "1"}], "]"}], "&"}], "/@", 
       RowBox[{"(", 
        RowBox[{"Range", "@", 
         RowBox[{"Length", "@", "newset"}]}], ")"}]}]}], ";", 
     "\[IndentingNewLine]", "\t", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"First", "@", 
        RowBox[{"Transpose", "@", "keep"}]}], ")"}], "[", 
      RowBox[{"[", 
       RowBox[{"All", ",", "1"}], "]"}], "]"}]}]}], "\[IndentingNewLine]", 
