     RowBox[{"(*", " ", 
      RowBox[{
      "Keeping", " ", "only", " ", "the", " ", "peak", " ", "from", " ", 
       "each", " ", "group"}], " ", "*)"}], "\[IndentingNewLine]", "\t", 
     RowBox[{"keep", "=", 
