  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "n", ",", "X", ",", "\[Delta]X", ",", "f", ",", "\[Delta]f", ",", "u", 
      ",", "x", ",", "pd", ",", "calc"}], "}"}], ",", " ", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"(*", 
