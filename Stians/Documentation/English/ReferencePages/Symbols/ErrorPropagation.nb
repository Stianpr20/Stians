     RowBox[{
      RowBox[{"{", 
       RowBox[{"X", ",", "\[Delta]X"}], "}"}], "=", 
      RowBox[{"Transpose", "@", 
       RowBox[{"{", "values", "}"}]}]}], ";", "\[IndentingNewLine]", "\t", 
     RowBox[{"f", "=", 
      RowBox[{"function", "@@", "X"}]}], ";", "\[IndentingNewLine]", 
