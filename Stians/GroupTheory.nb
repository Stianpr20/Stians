 RowBox[{
  RowBox[{"LinearCongruenceSolve", "[", 
   RowBox[{
   "\[Alpha]_Integer", ",", "\[Beta]_Integer", ",", "\[Gamma]_Integer"}], 
   "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "solutions", ",", "a", ",", "b", ",", "c", ",", "k", ",", "x", ",", "X", 
